{
  "id": "H8eW8uubGfEY",
  "name": "docs",
  "type": "app",
  "routines": {
    "init_tmux": "run tmux init docs",
    "attach_tmux": "run tmux attach docs",
    "terminate_tmux": "run tmux terminate docs",
    "claude_marketplace_add": "claude plugin marketplace add ./",
    "claude_marketplace_remove": "claude plugin marketplace remove ghostmind",
    "claude_plugin_install_utils": "claude plugin install utils@ghostmind",
    "claude_plugin_install_system": "claude plugin install system@ghostmind"
  },
  "tmux": {
    "sessions": [
      {
        "name": "docs",
        "windows": [
          {
            "name": "claude",
            "layout": "compact",
            "compact": {
              "type": "single",
              "panes": {
                "main": "Claude"
              }
            }
          },
          {
            "name": "shell",
            "layout": "compact",
            "compact": {
              "type": "single",
              "panes": {
                "main": "$SHELL"
              }
            }
          }
        ]
      }
    ]
  }
}
